<div class="sidenavbar">
  {{#each groupTitle}}
  	<a href="#">{{this}}</a>
  	{{/each}}
</div>
<div class="group-container">
<!-- views/partials/navbar.handlebars -->
{{#each msg}}
	<!-- CATEGORY HOLDER -->
	<div class="category-holder" id="{{this.title}}">
		<!-- HEADER -->
		<div class="info-heading">
			<h2><a href="/occupations/{{this.link}}">{{this.title}}</a></h2>
		</div>
		<!-- PARAGRAPH -->
		<div class="info-text">
			<p>{{this.description}}</p>
			<div class="occupation-stats">
				<p>Salary: {{this.average_salary}}</p>
				<p>Current Employment: {{this.employment}}</p>
				<p>Job Outlook: {{this.outlook}} new jobs</p>
			</div>
		</div>
	</div>
{{/each}}
</div>
<!-- FOR FLOATING PURPOSES -->
<div style="clear:both;"></div>